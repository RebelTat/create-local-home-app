{
  "name": "@google/create-local-home-app",
  "version": "0.2.1",
  "description": "Create Local Home SDK apps.",
  "bin": "./main.js",
  "author": "Google",
  "license": "Apache-2.0",
  "repository": "actions-on-google/create-local-home-app",
  "homepage": "https://github.com/actions-on-google/create-local-home-app#readme",
  "bugs": {
    "url": "https://github.com/actions-on-google/create-local-home-app/issues"
  },
  "scripts": {
    "compile": "tsc --noEmit",
    "build": "tsc",
    "prepare": "npm run build",
    "test": "ava test.ts",
    "lint": "tslint --project .",
    "start": "ts-node main.ts"
  },
  "dependencies": {
    "array.prototype.flatmap": "^1.2.3",
    "replace-in-file": "^6.1.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.11",
    "@types/yargs": "^15.0.5",
    "ava": "^3.8.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "index.js",
    "main.js",
    "npm-shrinkwrap.json",
    "app/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
